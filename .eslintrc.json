{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "block-scoped-var": "off",
        "brace-style": "off",
        "comma-dangle": "off",
        "func-names": "off",
        "indent": [
            "error",
            4
        ],
        "linebreak-style": "off",
        "no-param-reassign": "off",
        "no-plusplus": "off",
        "no-shadow": "off",
        "no-undef": "off",
        "no-use-before-define": "off",
        "no-var": "off",
        "object-curly-spacing": "off",
        "one-var": "off",
        "one-var-declaration-per-line": "off",
        "operator-linebreak": "off",
        "padded-blocks": "off",
        "prefer-arrow-callback": "off",
        "prefer-destructuring": "off",
        "prefer-template": "off",
        "quote-props": [
            "error",
            "always"
        ],
        "quotes": "off",
        "space-before-function-paren": "off",
        "vars-on-top": "off"
    }
}